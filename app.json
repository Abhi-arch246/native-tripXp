{
  "expo": {
    "name": "Trip XP",
    "slug": "trip-xp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher-web.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/hot-air-balloon.png",
        "backgroundImage": "./assets/images/ic_launcher-web.png",
        "monochromeImage": "./assets/images/ic_launcher-web.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/hot-air-balloon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/ic_launcher-web.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#1e146c",
          "dark": {
            "backgroundColor": "#1e146c"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "tsConfigPaths": true,
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
